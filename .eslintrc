{
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-strongly-recommended"
  ],
  "rules": {
    "vue/html-self-closing": "off",
    "vue/max-attributes-per-line": [
      "error",
      {
        "singleline": 4,
        "multiline": {
          "max": 1,
          "allowFirstLine": false
        }
      }
    ],
    "vue/no-mutating-props": "off",
    "vue/attribute-hyphenation": [
      "error",
      "always",
      {
        "ignore": ["viewBox"]
      }
    ]
  },
  "globals": {
  },
  "env": {
    "browser": true,
    "es6": true
  }
}
